<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_bg"
    tools:context=".accueil">


    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="191dp"
        android:layout_height="195dp"
        android:layout_marginBottom="148dp"
        android:orientation="vertical"
        app:cardBackgroundColor="@color/md_divider_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/pubbb"
                android:layout_width="match_parent"
                android:layout_height="145dp"
                app:srcCompat="@drawable/publicitei" />

            <Button
                android:id="@+id/button22"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#D3FFFFFF"
                android:backgroundTint="#E4FFFFFF"
                android:drawableTint="#C6FFFFFF"
                android:text="PUBLICTE" />

        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView2"
        android:layout_width="224dp"
        android:layout_height="195dp"
        android:layout_marginBottom="148dp"
        android:orientation="vertical"
        app:cardBackgroundColor="@color/md_divider_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <ImageView
                android:id="@+id/sortieeb"
                android:layout_width="wrap_content"
                android:layout_height="145dp"


                app:srcCompat="@drawable/sorchatt" />

            <Button
                android:id="@+id/button33"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#D3FFFFFF"
                android:backgroundTint="#E4FFFFFF"
                android:drawableTint="#C6FFFFFF"
                android:text="EXCURSION" />
        </LinearLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:layout_width="220dp"
        android:layout_height="193dp"
        android:orientation="vertical"
        app:cardBackgroundColor="@color/md_divider_white"
        app:layout_constraintBottom_toTopOf="@+id/cardView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="220dp"
            android:layout_height="196dp"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/mapboxxi"
                android:layout_width="match_parent"
                android:layout_height="138dp"


                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/hryz"
                tools:ignore="MissingConstraints" />

            <Button
                android:id="@+id/buttoo"
                android:layout_width="match_parent"
                android:layout_height="62dp"
                android:background="#D3FFFFFF"
                android:backgroundTint="#E4FFFFFF"
                android:drawableTint="#C6FFFFFF"
                android:text="EXCURSION" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/yrmaiil"
        android:layout_width="131dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:text="TextView"
        android:visibility="invisible"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.112"
        tools:visibility="invisible" />

    <androidx.cardview.widget.CardView
        android:layout_width="189dp"
        android:layout_height="193dp"
        android:orientation="vertical"
        app:cardBackgroundColor="@color/md_divider_white"
        app:layout_constraintBottom_toTopOf="@+id/cardView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/endroitidd"
                android:layout_width="match_parent"
                android:layout_height="137dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.1"
                app:layout_constraintStart_toStartOf="parent"
                app:srcCompat="@drawable/jamaeya"
                tools:layout_editor_absoluteY="230dp" />

            <Button
                android:id="@+id/button44"
                android:layout_width="match_parent"
                android:layout_height="71dp"
                android:background="#D3FFFFFF"
                android:backgroundTint="#E4FFFFFF"
                android:drawableTint="#C6FFFFFF"
                android:text="Places" />
        </LinearLayout>
    </androidx.cardview.widget.CardView>

</androidx.constraintlayout.widget.ConstraintLayout>